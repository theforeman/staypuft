<% if @destroyed %>
  $('#<%= dom_id(@subnet_type) %>').remove();
  $("#subnet_types .panel-body").append('<%=j render :partial => 'staypuft/subnet_types/subnet_type_pull',
      :object => @subnet_type,
      :locals => { :deployment => @deployment },
      :as => 'subnet_type' %>');
  $("#<%= dom_id(@subnet_type) %>").draggable({revert: 'invalid'});
<% end %>

<% if @warn.present? %>
  $.jnotify('<%=j @warn.html_safe %>', { type: "warning", sticky: true });
<% end %>
